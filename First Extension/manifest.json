{
    "manifest_version": 3,
    "name": "Random Image Extension",
    "version": "1.0",
    "description": "Displays a random image on popup",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": []
  }
  